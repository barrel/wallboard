{{! Template: Rooms Calendar }}
<section class="wallboard-calendar">
	<table>
		<thead>
			<tr>
				<th></th>
				<th>The Atrium</th>
				<th>The Garage</th>
				<th>The Cellar</th>
			</tr>
		</thead>
		<tbody>
			{{#hours}}
			<tr{{class}}>
				<td>{{hourprint}} <span class="meridiem">{{meridiem}}</span></td>
				
				{{#meetings_html}}{{{.}}}{{/meetings_html}}
			{{/hours}}
		</tbody>
	</table>
</section>
